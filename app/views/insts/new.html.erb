<div class="container">
  <div class="col-md-offset-3 col-md-6 col-md-offset-3 col-sm-10 ">
<p><%= notice %></p>
<br>
  <div class="thumbnail">
    <%= form_for(@inst) do |f| %>
        <div><%= f.label :タイトル %></div>
        <div><%= f.text_field :title %></div>
        <br>
        
        <div><%= f.label :内容 %></div>
        <div><%= f.text_field :content %></div>
        <br>
        
        <div><%= f.label :画像選択 %></div>
        <div><%= f.file_field :image %></div>
        <br>
        
        <div><%= f.submit %></div>
      <% end %>
    </div>
  </div>
</div>